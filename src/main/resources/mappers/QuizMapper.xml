<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hwanjin.hizini.domain.dao.QuizMapper">
	<select id="getData" resultType="QuizDTO">
		SELECT 
				ID
				, NAME
				, PASSWORD
				, ADDRESS
		FROM 
				QUIZ.TEST

		WHERE	
				NAME = ${name}
	</select>

</mapper>
 